<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_home_inspection_notice"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.ldnet.activity.homeInspectionManage.MainHomeInspectionActivity">
    <include layout="@layout/title_white_theme2"/>
    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="@color/gray_back"
        />
    <include layout="@layout/viewpager_title_complete"/>
    <android.support.v4.view.ViewPager
        android:id="@+id/viewpager_inspection"
        android:layout_width="match_parent"
        android:layout_height="match_parent">
    </android.support.v4.view.ViewPager>

</LinearLayout>
